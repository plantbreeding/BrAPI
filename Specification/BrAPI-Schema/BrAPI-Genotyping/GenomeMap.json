                    "type": [
                        "null",
                        "integer"
                    ]
                },
                "genomeMapDbId": {
                    "description": "The unique identifier for a `GenomeMap`",
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "mapName": {
